{
    "name": "louzet/composer-lock-file-parser",
    "description": "",
    "keywords": [
        "composer parser",
        "composer-lock-file-parser",
        "parser",
        "clfp"
    ],
    "homepage": "https://github.com/louzet/composer-lock-file-parser",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mickael Louzet",
            "email": "micklouzet@hotmail.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ComposerLockParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ComposerLockParser\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
